{
  "name": "litkit",
  "version": "0.0.0",
  "description": "Helpers for lit components",
  "keywords": [],
  "author": {
    "name": "Jakub Hajduk",
    "email": "devhajduk@gmail.com"
  },
  "type": "module",
  "scripts": {
    "dev": "tsdown --watch ./src/",
    "build": "tsdown",
    "format": "biome --check --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "ISC",
  "packageManager": "pnpm@10.12.4",
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@storybook/web-components-vite": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "lit": "catalog:"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./css-state": {
      "import": "./dist/css-state.mjs",
      "require": "./dist/css-state.cjs"
    },
    "./event-listener": {
      "import": "./dist/event-listener.mjs",
      "require": "./dist/event-listener.cjs"
    },
    "./generic-event-emitters": {
      "import": "./dist/generic-event-emitters.mjs",
      "require": "./dist/generic-event-emitters.cjs"
    },
    "./internals": {
      "import": "./dist/internals.mjs",
      "require": "./dist/internals.cjs"
    },
    "./options": {
      "import": "./dist/options.mjs",
      "require": "./dist/options.cjs"
    },
    "./roving-tabindex": {
      "import": "./dist/roving-tabindex.mjs",
      "require": "./dist/roving-tabindex.cjs"
    },
    "./slots": {
      "import": "./dist/slots.mjs",
      "require": "./dist/slots.cjs"
    },
    "./update": {
      "import": "./dist/update.mjs",
      "require": "./dist/update.cjs"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "vite": "^7.1.12"
  }
}
